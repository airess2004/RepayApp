<Alloy>
  <TableViewRow id="reimburseDetailRow" title="{title}" className="detailRow1">
  	<View id="innerView"  onClick="rowClick" onLongclick="rowLongClick">
  		<View id="avatarView" onClick="thumbPopUp">
    		<ImageView id="avatar" image="{urlImageSmall}" imageOri="{urlImageOriginal}" />
    	</View>
    	<View class="midView" >
    		<Label id="title" text="{name}" />
    		
    	</View>
    	<View class="rightView" layout="vertical">
    		<Label id="date" text="{receiptDate}"/>
    		<Label id="total" text="{amount}"/>
    		<Label id="status" text="{status}" />
    	</View>
    </View>
  </TableViewRow>
  
</Alloy>