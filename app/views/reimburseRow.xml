<Alloy>
  <TableViewRow id="reimburseRow">
  	<View id="innerView" layout="horizontal" onClick="rowClick" onLongclick="rowLongClick">
    	<ImageView class="avatar" onClick="thumbPopUp"/>
    	<Label id="title" text="{title}" />
    	<View class="rightView">
    		<Label id="status" text="{status}"/>
    		<Label id="total" text="{total}"/>
    	</View>
    </View>
  </TableViewRow>
</Alloy>